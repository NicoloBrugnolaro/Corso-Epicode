#include <stdio.h>

int main(){
    int num1 = 4;
    int num2 = 5;
    int addizione = num1 + num2
    printf("Addizione di %d + %d = &d", num1, num2, addizione);
    printf("sottrazione di %d + %d = %n", num1, num2, num1 - num2);
    printf ("Moltiplicazione di %d * %d = %d/n", num1, num2, num1 * num2);
    printf ("Divisione di %d / %d = %.2f/n", num1, num2, (float)num1 / (float)num2);
    return 0;
    }